#include<iostream>
using namespace std;
int multiply(int arr[], int len) {
   int i,temp=1;
   for(i=0;i<len;i++) {
      temp=temp*arr[i];
   }
   return temp;
}
int main() {
   int arr[]={1,2,3,4,5,6,7};
   int len=sizeof(arr)/sizeof(arr[0]);
   int value = multiply(arr,len);
   cout<<"value of array elements after multiplication :"<< value <<endl;
   return 0;
}
